<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <title>Super mock</title>
  <link rel="stylesheet" href="/css/menu.css">
  <link rel="stylesheet" href="/css/salary-query.css">
</head>
<body>
<main>
  <table>
    <thead>
      <tr>
        <th>employee_id</th>
        <th>amount</th>
        <th>from_date</th>
        <th>to_date</th>
      </tr>
    </thead>
    <tbody>
      {{#each employees}}
        <tr>
          <td>{{this.employee_id}}</td>
          <td>${{this.amount}}</td>
          <td>{{this.from_date}}</td>
          <td>{{this.to_date}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</main>
<img class="img-full" src="/img/000.png" alt="Image">
<img class="img-full" src="/img/001.jpg" alt="Image">
<img class="img-full" src="/img/002.jpg" alt="Image">
<img class="img-full" src="/img/003.png" alt="Image">
</body>
</html>
